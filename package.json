{
  "name": "dorf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "cap:sync": "npm run build && npx cap sync",
    "cap:open": "npx cap open android",
    "cap:run": "npx cap run android",
    "build:apk": "npm run cap:sync && cd android && ./gradlew assembleDebug",
    "build:apk:upload": "npm run build:apk && rclone copyto android/app/build/outputs/apk/debug/app-debug.apk \"gdrive:Dorf/builds/dorf.apk\" && rclone copyto android/app/build/outputs/apk/debug/app-debug.apk \"gdrive:Dorf/builds/dorf-$(date +%Y%m%d-%H%M).apk\"",
    "generate-assets": "node --import ./scripts/lib/register-loader.js scripts/generate-assets.js",
    "generate-battle-backgrounds": "node --import ./scripts/lib/register-loader.js scripts/generate-battle-backgrounds.js",
    "generate-region-maps": "node --import ./scripts/lib/register-loader.js scripts/generate-region-maps.js"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "pinia": "^3.0.4",
    "sharp": "^0.34.5",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "happy-dom": "^20.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.17"
  }
}
